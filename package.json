{
  "name": "@solid/identity-token-verifier",
  "version": "0.6.3",
  "description": "Verifies Solid access tokens via their WebID claim, and thus asserts ownership of WebIDs.",
  "license": "MIT",
  "keywords": [
    "Token verification",
    "Solid Identity",
    "Access Token",
    "DPoP",
    "OAuth"
  ],
  "contributors": [
    "Matthieu Bosquet <matthieu@cognithive.com> (https://github.com/matthieubosquet/)"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "audit": "npm audit --production --audit-level=high",
    "build": "tsc",
    "lint": "eslint --cache --fix --ignore-path .gitignore .",
    "lint:staged": "lint-staged",
    "prepublishOnly": "npm ci && pinst --disable && npm run build",
    "postpublish": "pinst --enable",
    "postinstall": "[[ -n \"$CI\" ]] || husky install",
    "test": "jest"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solid/identity-token-verifier.git"
  },
  "bugs": {
    "url": "https://github.com/solid/identity-token-verifier/issues"
  },
  "homepage": "https://github.com/solid/identity-token-verifier#readme",
  "devDependencies": {
    "@solid/eslint-config-base": "^0.2.2",
    "@tsconfig/node12": "^1.0.7",
    "@types/jest": "^26.0.20",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^12.19.12",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "cross-fetch": "^3.0.6",
    "jose": "^3.7.0",
    "lru-cache": "^6.0.0",
    "n3": "^1.8.1",
    "nmspc": "^0.2.4",
    "rdf-data-factory": "^1.0.4",
    "rdf-dereference": "^1.7.0",
    "rdf-parse": "^1.7.0",
    "rdf-store-stream": "^1.1.0",
    "ts-guards": "^0.5.1"
  }
}
